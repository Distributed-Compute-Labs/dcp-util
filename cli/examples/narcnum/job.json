  },

  "options":    { "get": {}, "put": {} },
  "type":       "static",
  "dependencies": {},
  "command":    {
   "worker": "worker.js",
   "sort": 0,
   "sliceByN": 1,
   "slices" : [
     {
       "data" : [1,1000,1]
     }
   ],
   
    "options" : {
      "input" : {
        "dataType" : "64"
      }
    }
  }

    "test": "peter -v tests"
  },
  "dependencies": {
    "dcp-client": "git+ssh://git@gitlab.com/Distributed-Compute-Protocol/dcp-client.git#feature/dcp-1578-dcp-util-scripts-do-not-work-with-scheduler-v4",
    "debug": "4.3.1"
  },
  "devDependencies": {
    "concat-stream": "2.0.0",
    "cross-spawn": "7.0.3",
    "peter": "git+ssh://git@gitlab.com/Distributed-Compute-Protocol/peter.git#develop",
    "zora": "4.0.2"
  }
}
